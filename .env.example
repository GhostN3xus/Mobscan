# Mobscan Environment Variables
# Copy this file to .env and update with your actual values

# API Security
# Generate a secure SECRET_KEY with: python -c 'import secrets; print(secrets.token_urlsafe(32))'
SECRET_KEY=your-secret-key-here-change-this

# Token expiry in minutes (default: 30)
TOKEN_EXPIRY_MINUTES=30

# Database Configuration
POSTGRES_DB=mobscan
POSTGRES_USER=mobscan
POSTGRES_PASSWORD=change-this-secure-password

# Database URL for SQLAlchemy
DATABASE_URL=postgresql://mobscan:change-this-secure-password@postgres:5432/mobscan

# Redis Configuration
REDIS_HOST=redis
REDIS_PORT=6379
REDIS_DB=0
REDIS_PASSWORD=

# pgAdmin Configuration
PGADMIN_DEFAULT_EMAIL=admin@mobscan.dev
PGADMIN_DEFAULT_PASSWORD=change-this-admin-password

# Application Settings
MOBSCAN_LOG_LEVEL=INFO
MOBSCAN_PARALLEL_WORKERS=4
MOBSCAN_TIMEOUT=7200

# CORS Configuration
# Comma-separated list of allowed origins
CORS_ALLOWED_ORIGINS=http://localhost:3000,http://localhost:8080

# MobSF Configuration
MOBSF_API_URL=http://mobsf:8000
MOBSF_API_KEY=

# mitmproxy Configuration
MITMPROXY_HOST=mitmproxy
MITMPROXY_PORT=8080

# Frida Configuration
FRIDA_HOST=frida-server
FRIDA_PORT=27042
